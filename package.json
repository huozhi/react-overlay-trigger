  "author": "huozhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huozhi/react-overlay-trigger/issues"
  },
  "homepage": "https://github.com/huozhi/react-overlay-trigger#readme",
  "peerDependencies": {
    "react": "^19.0.0-beta.0",
    "react-dom": "^19.0.0-beta.0"
  },
  "devDependencies": {
    "bunchee": "^5.1.2",
    "next": "^14.2.3",
    "react": "^19.0.0-beta.0",
    "react-dom": "^19.0.0-beta.0",
    "react-overlay-trigger": "link:./",
    "sugar-high": "^0.5.2"
