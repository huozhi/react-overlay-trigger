  "files": [
    "dist",
    "type.d.ts",
    "*.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huozhi/react-overlay-trigger.git"
  },
  "keywords": [
    "react",
    "overlay",
    "positioned",
    "trigger",
    "tooltip"
