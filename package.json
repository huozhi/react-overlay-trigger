{
  "name": "react-overlay-trigger",
  "version": "0.3.1",
  "description": "react positioned overlay component",
  "main": "lib/index.js",
  "module": "lib/index.module.js",
  "types": "type.d.ts",
  "scripts": {
    "clean:docs": "rm -rf docs/dist",
    "build:docs": "npm run clean:docs && NODE_ENV=production webpack --colors --progress --config docs/webpack.config.js",
    "deploy:docs": "npm run build:docs && gh-pages -d docs/dist",
    "prepublishOnly": "rm -rf dist && npm run build",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "NODE_ENV=development webpack-dev-server --hot --progress --colors --content-base docs --config docs/webpack.config.js"
  },
  "files": [
    "*.md",
    "lib",
    "src",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huozhi/react-overlay-trigger.git"
  },
  "keywords": [
    "react",
    "overlay",
    "positioned",
    "trigger",
    "tooltip"
  ],
  "author": "huozhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huozhi/react-overlay-trigger/issues"
  },
  "homepage": "https://github.com/huozhi/react-overlay-trigger#readme",
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.6",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^1.2.1",
    "postcss-nested": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^8.2.5",
    "rollup-plugin-node-resolve": "^3.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
