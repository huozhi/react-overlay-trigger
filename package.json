{
  "name": "react-overlay-trigger",
  "version": "1.0.4",
  "description": "react positioned overlay component",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/type.d.ts",
  "scripts": {
    "clean:docs": "rm -rf docs/dist",
    "build:docs": "npm run build && npm run clean:docs && NODE_ENV=production webpack --colors --progress --config docs/webpack.config.js",
    "prepublishOnly": "rm -rf dist && npm run build",
    "build": "bunchee src/index.js && cp type.d.ts dist/type.d.ts",
    "dev": "yarn build --watch",
    "start": "NODE_ENV=development webpack-dev-server --hot --progress --colors --content-base docs --config docs/webpack.config.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huozhi/react-overlay-trigger.git"
  },
  "keywords": [
    "react",
    "overlay",
    "positioned",
    "trigger",
    "tooltip"
  ],
  "author": "huozhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huozhi/react-overlay-trigger/issues"
  },
  "homepage": "https://github.com/huozhi/react-overlay-trigger#readme",
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "devDependencies": {
    "babel-loader": "^8.0.5",
    "bunchee": "1.5.1",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^1.2.1",
    "postcss-nested": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
