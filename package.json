{
  "name": "react-overlay-trigger",
  "version": "2.0.1",
  "description": "react positioned overlay component",
  "main": "./dist/index.js",
  "exports": {
    "import": "./dist/index.mjs",
    "default": "./dist/index.js"
  },
  "types": "./type.d.ts",
  "scripts": {
    "prepublishOnly": "rm -rf dist && pnpm run build:lib",
