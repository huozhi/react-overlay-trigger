{
  "name": "react-overlay-trigger",
  "version": "1.2.0",
  "description": "react positioned overlay component",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "module": "./dist/index.esm.js"
  },
  "types": "./type.d.ts",
  "scripts": {
    "prepublishOnly": "rm -rf dist && npm run build",
    "build:watch": "pnpm build:lib --watch",
    "build:lib": "bunchee",
    "docs:clean": "rm -rf docs/dist",
    "docs:build": "pnpm build:lib && pnpm next build docs",
    "dev": "pnpm next docs"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huozhi/react-overlay-trigger.git"
  },
  "keywords": [
    "react",
    "overlay",
    "positioned",
    "trigger",
    "tooltip"
  ],
  "author": "huozhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huozhi/react-overlay-trigger/issues"
  },
  "homepage": "https://github.com/huozhi/react-overlay-trigger#readme",
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "bunchee": "^3.4.1",
    "next": "^13.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-overlay-trigger": "link:./"
  },
  "dependencies": {
    "sugar-high": "^0.4.6"
  }
}
